{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "types": ["react", "react-dom"],
    "lib": ["dom", "ES2015", "ES2016", "ES2017"],
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "themes": ["themes/index.ts"]
    }
  },
  "include": [
   "src"
  ],
  "exclude": ["node_modules"]
}
